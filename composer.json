{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "friendsofsymfony/rest-bundle": "^2.3",
        "friendsofsymfony/user-bundle": "^2.1",
        "league/tactician-bundle": "^1.1",
        "league/tactician-doctrine": "^1.1",
        "nelmio/api-doc-bundle": "^3.1",
        "ramsey/uuid": "^3.7",
        "sensio/framework-extra-bundle": "^5.1",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/doctrine-orm-admin-bundle": "^3.4",
        "sonata-project/google-authenticator": "^2.0",
        "sonata-project/media-bundle": "^3.6",
        "sonata-project/user-bundle": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "^3.3",
        "symfony/debug-bundle": "^3.3",
        "symfony/debug-pack": "^1.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^3.3",
        "symfony/framework-bundle": "^3.3",
        "symfony/lts": "^3",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^3.3",
        "symfony/security-csrf": "^3.3",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "^3.3",
        "symfony/twig-bundle": "^3.3",
        "symfony/validator": "^3.3",
        "symfony/web-server-bundle": "^3.3",
        "symfony/yaml": "^3.3"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.2",
        "hautelook/alice-bundle": "^2.0@beta",
        "nelmio/alice": "^3.0@beta",
        "symfony/dotenv": "^3.3",
        "theofidry/alice-data-fixtures": "^1.0@beta"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "make cache-warmup": "script",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "cache:clear": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/twig-bundle": "<3.3",
        "symfony/debug": "<3.3"
    },
    "extra": {
        "symfony": {
            "id": "01BN83E808M1W0NNSEDZGZGR1D",
            "allow-contrib": true
        }
    }
}
